{
  "extends":"@ayu-mu/tsconfig/tsconfig.base.json",
  "compilerOptions": {
    "outDir": "temp/src",
    "rootDir": "src",
    "declaration": true,
    "emitDeclarationOnly": true,
    "composite": true,
    "types": ["node"],
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"]
    }
  },
  "include": ["src/**/*.ts", "src/**/*.vue"],
  "exclude": ["dist", "node_modules", "**/*.test.ts", "**/__tests__/*"]
}
