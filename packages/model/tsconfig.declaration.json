{
  "extends":"@ayu-mu/tsconfig/tsconfig.base.json",
  "compilerOptions": {
    "moduleResolution": "bundler",
    "baseUrl": ".",
    "rootDir": ".",
    "paths": {
      "@/*": [
        "src/*"
      ]
    },
    "declaration": true,
    "emitDeclarationOnly": true,
    "outDir": "temp",
    "stripInternal": true
  },
  "include": ["./**/.vue", "./src/**/*.ts"]
}
